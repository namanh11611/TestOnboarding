<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layoutFirstRunFirst"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textFilterAdsDescription"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="34dp"
        android:layout_marginHorizontal="33dp"
        android:text="Loc cac quang cao gay gian doan khi ban luot web"
        android:textColor="#444545"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textFilterAdsTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginHorizontal="33dp"
        android:text="Loc quang cao"
        android:textColor="#2A2B2B"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@id/textFilterAdsDescription"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.motion.widget.MotionLayout
        android:id="@+id/layoutFilterAdsBannerWrapper"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layoutDescription="@xml/coccoc_first_run_first_scene"
        app:layout_constraintBottom_toTopOf="@id/textFilterAdsTitle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.5" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineVertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />

        <ImageView
            android:id="@+id/iconSpeaker"
            android:layout_width="46dp"
            android:layout_height="46dp"
            android:layout_marginEnd="104dp"
            android:layout_marginTop="9dp"
            android:scaleType="centerInside"
            android:src="@drawable/ic_coccoc_first_run_speaker"
            android:visibility="invisible"
            app:layout_constraintTop_toBottomOf="@id/guidelineHorizontal"
            app:layout_constraintEnd_toStartOf="@id/guidelineVertical" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutFilterAdsBanner"
            android:layout_width="230dp"
            android:layout_height="242dp"
            android:background="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layoutFilterAdsSave"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="33dp"
                android:background="#F5F5F5"
                android:padding="16dp"
                app:layout_constraintBottom_toBottomOf="parent">

                <TextView
                    android:id="@+id/textSaveData"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="93.1MB"
                    android:textColor="#444545"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textSaveDataDes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Du lieu tiet kiem"
                    android:textColor="#6E7171"
                    android:textSize="10sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textSaveData" />

                <TextView
                    android:id="@+id/textSaveTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="82 giay"
                    android:textColor="#444545"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toEndOf="@id/guidelineSave"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textSaveTimeDes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Thoi gian tiet kiem"
                    android:textColor="#6E7171"
                    android:textSize="10sp"
                    app:layout_constraintStart_toEndOf="@id/guidelineSave"
                    app:layout_constraintTop_toBottomOf="@id/textSaveTime" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guidelineSave"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.58" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <FrameLayout
            android:id="@+id/layoutFilterAdsSwitch"
            android:layout_width="214dp"
            android:layout_height="48dp"
            android:layout_marginBottom="65dp"
            android:background="#F5F5F5"
            app:layout_constraintBottom_toTopOf="@id/guidelineHorizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textFilterAdsSwitch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Loc quang cao"
            app:layout_constraintBottom_toBottomOf="@id/layoutFilterAdsSwitch"
            app:layout_constraintStart_toStartOf="@id/layoutFilterAdsSwitch"
            app:layout_constraintTop_toTopOf="@id/layoutFilterAdsSwitch" />

        <androidx.appcompat.widget.SwitchCompat
            android:id="@+id/switchFilterAds"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toBottomOf="@id/layoutFilterAdsSwitch"
            app:layout_constraintEnd_toEndOf="@id/layoutFilterAdsSwitch"
            app:layout_constraintTop_toTopOf="@id/layoutFilterAdsSwitch" />

        <ImageView
            android:id="@+id/iconShieldOff"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="36dp"
            android:src="@drawable/ic_coccoc_first_run_shield_off"
            app:layout_constraintStart_toStartOf="@id/layoutFilterAdsBanner"
            app:layout_constraintTop_toBottomOf="@id/layoutFilterAdsSwitch" />

        <ImageView
            android:id="@+id/iconShieldOn"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="36dp"
            android:src="@drawable/ic_coccoc_first_run_shield_on"
            android:visibility="invisible"
            app:layout_constraintStart_toStartOf="@id/layoutFilterAdsBanner"
            app:layout_constraintTop_toBottomOf="@id/layoutFilterAdsSwitch" />

        <TextView
            android:id="@+id/textFilterNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="84dp"
            android:text="19,234"
            android:textColor="#000000"
            android:textSize="34sp"
            app:layout_constraintBottom_toTopOf="@id/guidelineHorizontal"
            app:layout_constraintStart_toStartOf="@id/layoutFilterAdsBanner" />

        <TextView
            android:id="@+id/textFilter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Quang cao da loc"
            android:textColor="#444545"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@id/textFilterNumber"
            app:layout_constraintTop_toBottomOf="@id/textFilterNumber" />

        <ImageView
            android:id="@+id/iconVideo"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginBottom="113dp"
            android:scaleType="centerInside"
            android:src="@drawable/ic_coccoc_first_run_video_ads"
            android:visibility="invisible"
            app:layout_constraintBottom_toTopOf="@id/guidelineHorizontal"
            app:layout_constraintEnd_toEndOf="@id/layoutFilterAdsBanner"
            app:layout_constraintStart_toStartOf="@id/layoutFilterAdsBanner" />

        <ImageView
            android:id="@+id/iconTv"
            android:layout_width="1dp"
            android:layout_height="1dp"
            android:scaleType="centerInside"
            android:src="@drawable/ic_coccoc_first_run_tv_ads"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="@id/layoutFilterAdsBanner"
            app:layout_constraintEnd_toEndOf="@id/layoutFilterAdsBanner"
            app:layout_constraintStart_toEndOf="@id/layoutFilterAdsBanner"
            app:layout_constraintTop_toBottomOf="@id/layoutFilterAdsBanner" />

        <TextView
            android:id="@+id/text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="40dp"
            android:text="Hello1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:alpha="0"
            android:text="Hello2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            android:text="Hello3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.motion.widget.MotionLayout>

</androidx.constraintlayout.widget.ConstraintLayout>